/* pages/mvlist/mvlist.wxss */
@import "/style/iconfont.wxss";
page{
  padding: 0 20rpx;
  box-sizing: border-box;
}
.list{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.list .item{
  width: 345rpx;
  /* background-color: orangered; */
  margin-bottom: 15rpx;
}
.list .item .imgbox{
  width: 100%;
  height: 200rpx;
  position: relative;
  /* overflow: hidden; */
}
.list .item .imgbox > image{
  width: 100%;
  /* height: 100%; */
  /* position: absolute; */
}
.list .item .imgbox .iconfont{
  position: absolute;
  color: white;
  bottom: 5%;
  left: 5%;
  font-size: 30rpx;
}
.list .item .desc{
  font-size: 28rpx;
}

